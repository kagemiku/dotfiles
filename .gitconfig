[user]
	name = KAGE
	email = kagemiku@gmail.com
[color]
	diff = auto
	status = auto
	branch = auto
	interactive = auto
[core]
	excludesfile = /Users/kage/.gitignore_global
	editor = /usr/local/bin/vim
[filter "lfs"]
	clean = git-lfs clean %f
	smudge = git-lfs smudge %f
	required = true
[alias]
	co = checkout
	diffs = diff --cached
	graph = log --graph --date=short --pretty=format:'%Cgreen%h %Creset%cd %Cblue%cn %Cred%d %Creset%s'
	st = status
	sb = status -sb
	cm = commit
